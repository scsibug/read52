<div class="span-24 last">
   <h1>Books</h1>
</div>
<div class="span-24 last">
<div class="span-4"><strong>Cover</strong></div>
<div class="span-5"><strong>Title</strong></div>
<div class="span-5"><strong>ASIN/ISBN</strong></div>
<div class="span-5"><strong>EAN/ISBN-13</strong></div>
<div class="span-5 last"><strong>Author</strong></div>

<% for (ix in books) { 
   book = books[ix];
%>
<div class="container">
<div class="span-4"><img src="<%= book.amz_img_small%>"></div>
<div class="span-5"><a href="/book/<%= book.ean %>"><%= book.title %></a></div>
<div class="span-5"><%= book.asin %></div>
<div class="span-5"><%= book.ean %></div>
<div class="span-5 last"><%= book.author %></div>
</div>
<% } %>

